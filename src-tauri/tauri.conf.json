{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ManyMany.dev",
  "version": "0.1.0",
  "identifier": "com.jasonzhou.manymany",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ManyMany.dev",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Capability for main window",
          "windows": ["main"],
          "permissions": [
            "core:default",
            "core:event:allow-listen",
            "core:event:allow-emit",
            "dialog:allow-open",
            "dialog:default"
          ]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": "Developer ID Application: Meta XYZ Pty Limited (6X48X77HD6)",
      "hardenedRuntime": true,
      "entitlements": null
    }
  },
  "updater": {
    "active": true,
    "endpoints": [
      "https://github.com/JayZeeDesign/ManyMany.dev/releases/latest/download/latest.json"
    ],
    "dialog": true,
    "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFEOUFBNTExQkZCOENDNzcKUldSM3pMaS9FYVdhSFJhNU9PclFOWWRFdmtLaWxJOEcrckUvYnM3RFpQUkpQL0FwdGRQMmJqZ0oK"
  }
}
